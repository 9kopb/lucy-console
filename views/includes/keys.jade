.col-xs-12
  h3.text-regular
    span Credentials
  #KeyInputs
    .key.form-group(ng-repeat="input in keyInputs")
      label {{ input.label || input.name }}
      input.input-sm.form-control(type="text", ng-model="keys[input.name]", ng-change="keyChanged()")
    .form-group
      input(type="checkbox" ng-model="checks.saveKeys" ng-change="saveChanged()")
      label &nbsp;&nbsp;Remember Keys

